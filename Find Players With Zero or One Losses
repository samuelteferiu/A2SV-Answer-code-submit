class Solution:
    def findWinners(self, matches: List[List[int]]) -> List[List[int]]:
        losers = collections.Counter()
        players = set()
        for winner, loser in matches:
            players.add(winner)
            players.add(loser)
            losers[loser] += 1
        noloss = []
        once = []
        for p in sorted(players):
            if losers[p] == 0:
                noloss.append(p)
            elif losers[p] == 1:
                once.append(p)

        return [noloss, once]
