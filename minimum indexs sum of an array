class Solution:
    def findRestaurant(self, list1: List[str], list2: List[str]) -> List[str]:
        ans = []
        mins = float('inf')
        for i, val in enumerate(list1):
            if val in list2:
                j = list2.index(val)
                if i + j < mins:
                    mins = i + j
                    ans = [val]
                elif i + j == mins:
                    ans.append(val)
        return ans
