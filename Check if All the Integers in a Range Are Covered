class Solution:
    def isCovered(self, ranges: List[List[int]], left: int, right: int) -> bool:
        cov=set()
        for i in ranges:
            x,y=i
            for j in range(x,y+1):
                cov.add(j)
        for k in range(left,right+1):
            if k not in cov:
                return False
        return True


            

        
